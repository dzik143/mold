TEST "typeof(imm)"
print "typeof('text'):", typeof('text')
print "typeof(1234)  :", typeof(1234)
print "typeof(-1234) :", typeof(-1234)
print "typeof(3.14)  :", typeof(3.14)
print "typeof(true)  :", typeof(true)
print "typeof(false) :", typeof(false)
print "typeof([])    :", typeof([])
print "typeof({})    :", typeof({})

EXPECTED_OUTPUT:
typeof('text'): string
typeof(1234)  : integer
typeof(-1234) : integer
typeof(3.14)  : float
typeof(true)  : boolean
typeof(false) : boolean
typeof([])    : array
typeof({})    : map
