TEST "Bug: Destroyed registers inside for keys loop"

function foo(node) -> rv
  rv = {}
  for keys propertyId values propertyValue in node
    print rv
    rv{propertyId} = propertyValue
  endfor
endfunction

src = {'one': 1, 'two': 2, 'three': 3}
dst = foo(src)

print dst

EXPECTED_OUTPUT:
{}
{'one': 1}
{'one': 1, 'two': 2}
{'one': 1, 'two': 2, 'three': 3}
